# Example environment variables for the SAP HANA CDC pipeline implementation

# SAP HANA Connection
SAP_HANA_HOST=localhost
SAP_HANA_PORT=30015
SAP_HANA_USER=SYSTEM
SAP_HANA_PASSWORD=password
SAP_HANA_DATABASE=HDB

# CDC Configuration
CDC_SCHEMA=SYSTEM
CDC_TABLES=*
CDC_POLL_INTERVAL=5000
CDC_BATCH_SIZE=1000

# Kafka Configuration (for streaming)
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_TOPIC=sap-hana-cdc
KAFKA_CLIENT_ID=sap-hana-cdc-client

# Logging
LOG_LEVEL=INFO

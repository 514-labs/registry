{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "brandId": {
      "type": "integer",
      "description": "Unique identifier for the brand"
    },
    "brandName": {
      "type": "string",
      "description": "Name of the brand"
    },
    "externalCatalogId": {
      "type": ["string", "null"],
      "description": "External catalog identifier"
    }
  },
  "required": ["brandId", "brandName"]
}
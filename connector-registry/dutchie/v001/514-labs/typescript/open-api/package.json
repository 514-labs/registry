{
  "name": "@workspace/connector-dutchie",
  "version": "0.0.0",
  "private": true,
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./config": {
      "types": "./dist/src/config.d.ts",
      "default": "./dist/src/config.js"
    },
    "./client": {
      "types": "./dist/src/client.d.ts",
      "default": "./dist/src/client.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:integration": "jest --config jest.integration.cjs",
    "generate:types": "openapi-typescript ./schemas/files/dutchie-openapi.json -o src/generated/dutchie.ts"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@connector-factory/core": "^0.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.12",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "jest": "^29.7.0",
    "nock": "^13.5.4",
    "openapi-typescript": "^7.4.2",
    "ts-jest": "29.1.2",
    "typescript": "^5.6.3"
  }
}

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Google Analytics Data API v1beta - audienceExports.get Endpoint",
  "description": "Endpoint for retrieving an audience export for a Google Analytics 4 property",
  "type": "object",
  "properties": {
    "request": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "Required. The audience export resource name",
          "pattern": "^properties/[^/]+/audienceExports/[^/]+$"
        }
      },
      "required": ["name"]
    },
    "response": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "The audience export resource name"
        },
        "audience": {
          "type": "string",
          "description": "The audience resource name"
        },
        "audienceDisplayName": {
          "type": "string",
          "description": "The descriptive display name for this audience"
        },
        "dimensions": {
          "type": "array",
          "description": "The dimensions requested and displayed",
          "items": {
            "type": "object",
            "properties": {
              "dimensionName": { "type": "string" }
            }
          }
        },
        "state": {
          "type": "string",
          "enum": ["STATE_UNSPECIFIED", "CREATING", "ACTIVE", "FAILED"],
          "description": "The current state of this audience export"
        },
        "beginCreatingTime": {
          "type": "string",
          "format": "date-time",
          "description": "The time when CreateAudienceExport was called"
        },
        "creationQuotaTokensCharged": {
          "type": "integer",
          "description": "The total quota tokens charged during creation"
        },
        "rowCount": {
          "type": "integer",
          "description": "The total number of rows in the export"
        },
        "errorMessage": {
          "type": "string",
          "description": "Error message is populated when an audience export fails"
        },
        "percentageCompleted": {
          "type": "number",
          "description": "The percentage completed"
        }
      }
    }
  },
  "metadata": {
    "apiType": "endpoint",
    "httpMethod": "GET",
    "apiPath": "v1beta/{name=properties/*/audienceExports/*}"
  }
}

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Google Analytics Data API v1beta - runReport Endpoint",
  "description": "Endpoint for running a report on a Google Analytics 4 property",
  "type": "object",
  "properties": {
    "request": {
      "type": "object",
      "properties": {
        "property": {
          "type": "string",
          "description": "A Google Analytics 4 property identifier"
        },
        "dimensions": {
          "type": "array",
          "description": "The dimensions requested and displayed",
          "items": { "$ref": "#/definitions/Dimension" }
        },
        "metrics": {
          "type": "array",
          "description": "The metrics requested and displayed",
          "items": { "$ref": "#/definitions/Metric" }
        },
        "dateRanges": {
          "type": "array",
          "description": "Date ranges of data to read",
          "items": { "$ref": "#/definitions/DateRange" }
        },
        "dimensionFilter": {
          "description": "Dimension filters allow you to ask for only specific dimension values in the report",
          "$ref": "#/definitions/FilterExpression"
        },
        "metricFilter": {
          "description": "The filter clause of metrics",
          "$ref": "#/definitions/FilterExpression"
        },
        "offset": {
          "type": "string",
          "format": "int64",
          "description": "The row count of the start row"
        },
        "limit": {
          "type": "string",
          "format": "int64",
          "description": "The number of rows to return"
        },
        "metricAggregations": {
          "type": "array",
          "description": "Aggregation of metrics",
          "items": { "$ref": "#/definitions/MetricAggregation" }
        },
        "orderBys": {
          "type": "array",
          "description": "Specifies how rows are ordered in the response",
          "items": { "$ref": "#/definitions/OrderBy" }
        },
        "currencyCode": {
          "type": "string",
          "description": "A currency code in ISO4217 format"
        },
        "cohortSpec": {
          "description": "Cohort group associated with this request",
          "$ref": "#/definitions/CohortSpec"
        },
        "keepEmptyRows": {
          "type": "boolean",
          "description": "If false, rows with metrics being 0 will be omitted from results"
        },
        "returnPropertyQuota": {
          "type": "boolean",
          "description": "Toggles whether to return the current state of the Property's quota"
        },
        "comparisons": {
          "type": "array",
          "description": "Optional. The configuration of comparisons requested and displayed",
          "items": { "$ref": "#/definitions/Comparison" }
        }
      },
      "required": ["property"]
    },
    "response": {
      "$ref": "#/definitions/RunReportResponse"
    }
  },
  "definitions": {
    "RunReportResponse": {
      "type": "object",
      "properties": {
        "dimensionHeaders": {
          "type": "array",
          "items": { "$ref": "#/definitions/DimensionHeader" }
        },
        "metricHeaders": {
          "type": "array",
          "items": { "$ref": "#/definitions/MetricHeader" }
        },
        "rows": {
          "type": "array",
          "items": { "$ref": "#/definitions/Row" }
        },
        "totals": {
          "type": "array",
          "items": { "$ref": "#/definitions/Row" }
        },
        "maximums": {
          "type": "array",
          "items": { "$ref": "#/definitions/Row" }
        },
        "minimums": {
          "type": "array",
          "items": { "$ref": "#/definitions/Row" }
        },
        "rowCount": { "type": "integer" },
        "metadata": { "$ref": "#/definitions/ResponseMetaData" },
        "propertyQuota": { "$ref": "#/definitions/PropertyQuota" },
        "kind": { "type": "string" }
      }
    },
    "Dimension": { "type": "object" },
    "Metric": { "type": "object" },
    "DateRange": { "type": "object" },
    "FilterExpression": { "type": "object" },
    "MetricAggregation": { "type": "string" },
    "OrderBy": { "type": "object" },
    "CohortSpec": { "type": "object" },
    "Comparison": { "type": "object" },
    "DimensionHeader": { "type": "object" },
    "MetricHeader": { "type": "object" },
    "Row": { "type": "object" },
    "ResponseMetaData": { "type": "object" },
    "PropertyQuota": { "type": "object" }
  },
  "metadata": {
    "apiType": "endpoint",
    "httpMethod": "POST",
    "apiPath": "v1beta/{property=properties/*}:runReport"
  }
}

{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Google Analytics Data API v1beta - getMetadata Endpoint",
  "description": "Endpoint for retrieving metadata for dimensions and metrics available in reporting methods",
  "type": "object",
  "properties": {
    "request": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "The resource name of the metadata to retrieve",
          "pattern": "^properties/[^/]+/metadata$"
        }
      },
      "required": ["name"]
    },
    "response": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "description": "Resource name of this metadata"
        },
        "dimensions": {
          "type": "array",
          "description": "The dimension descriptions",
          "items": { "$ref": "#/definitions/DimensionMetadata" }
        },
        "metrics": {
          "type": "array",
          "description": "The metric descriptions",
          "items": { "$ref": "#/definitions/MetricMetadata" }
        },
        "comparisons": {
          "type": "array",
          "description": "The comparison descriptions",
          "items": {
            "type": "object",
            "properties": {
              "apiName": { "type": "string" },
              "uiName": { "type": "string" },
              "description": { "type": "string" }
            }
          }
        }
      }
    }
  },
  "definitions": {
    "DimensionMetadata": { "type": "object" },
    "MetricMetadata": { "type": "object" }
  },
  "metadata": {
    "apiType": "endpoint",
    "httpMethod": "GET",
    "apiPath": "v1beta/{name=properties/*/metadata}"
  }
}

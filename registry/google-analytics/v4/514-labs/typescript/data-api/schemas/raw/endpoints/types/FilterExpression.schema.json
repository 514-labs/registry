{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Google Analytics Data API v1beta - FilterExpression Type",
  "description": "To express dimension or metric filters, create a hierarchy of filters using logical operators",
  "type": "object",
  "properties": {
    "andGroup": {
      "type": "object",
      "description": "The FilterExpressions in andGroup have an AND relationship",
      "properties": {
        "expressions": {
          "type": "array",
          "items": { "$ref": "#/definitions/FilterExpression" }
        }
      }
    },
    "orGroup": {
      "type": "object",
      "description": "The FilterExpressions in orGroup have an OR relationship",
      "properties": {
        "expressions": {
          "type": "array",
          "items": { "$ref": "#/definitions/FilterExpression" }
        }
      }
    },
    "notExpression": {
      "description": "The FilterExpression is NOT of notExpression",
      "$ref": "#/definitions/FilterExpression"
    },
    "filter": {
      "type": "object",
      "description": "A primitive filter",
      "properties": {
        "fieldName": { "type": "string" },
        "stringFilter": {
          "type": "object",
          "properties": {
            "matchType": {
              "type": "string",
              "enum": [
                "MATCH_TYPE_UNSPECIFIED",
                "EXACT",
                "BEGINS_WITH",
                "ENDS_WITH",
                "CONTAINS",
                "FULL_REGEXP",
                "PARTIAL_REGEXP"
              ]
            },
            "value": { "type": "string" },
            "caseSensitive": { "type": "boolean" }
          }
        },
        "inListFilter": {
          "type": "object",
          "properties": {
            "values": {
              "type": "array",
              "items": { "type": "string" }
            },
            "caseSensitive": { "type": "boolean" }
          }
        },
        "numericFilter": {
          "type": "object",
          "properties": {
            "operation": {
              "type": "string",
              "enum": [
                "OPERATION_UNSPECIFIED",
                "EQUAL",
                "LESS_THAN",
                "LESS_THAN_OR_EQUAL",
                "GREATER_THAN",
                "GREATER_THAN_OR_EQUAL"
              ]
            },
            "value": {
              "type": "object",
              "properties": {
                "int64Value": { "type": "string" },
                "doubleValue": { "type": "number" }
              }
            }
          }
        },
        "betweenFilter": {
          "type": "object",
          "properties": {
            "fromValue": { "type": "object" },
            "toValue": { "type": "object" }
          }
        }
      }
    }
  },
  "definitions": {
    "FilterExpression": { "$ref": "#" }
  },
  "metadata": {
    "apiType": "type",
    "description": "Defines filters for dimensions and metrics in reports"
  }
}
